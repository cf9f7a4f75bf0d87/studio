<%- include bheader.ejs%>

<div class="container">
    <%if(typeof(docs)== 'object') docs.forEach(function(v){ %>
    <div class="row"><div class="col-md-12">
       <p><%=v%></p>
      </div></div>
   <% });%>

</div>
<div class="container">
    <div class="row">
        <form role="form" class="form-horizontal" action="" method="post">
            <div class="form-group col-md-4">
                <label class="control-label" for="etitle">标题</label>
                <input type="text" class="form-control" id="etitle" name="etitle">
            </div>

            <div class="form-group col-md-4">
                <label class="control-label" for="econtent">大事件内容</label>
                <input type="text" class="form-control" id="econtent" name="econtent">
            </div>
            <div class="form-group col-md-4">
                <label class="control-label" for="epics">新闻配图</label>
                <input type="text" class="form-control" id="epics" name="epics">
            </div>
            <div class="form-group col-md-4">
                <label class="control-label" for="epubTime">事件时间</label>
                <input type="date" class="form-control" id="epubTime" name="epubTime">
            </div>
            <div class="col-md-5">
                <button type="submit" class="btn btn-default center-block">提交</button>
            </div>
        </form>
    </div>
</div>

<%- include bfooter.ejs%>